<p id="notice"><%= notice %></p>

<h1>Tracks</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>X</th>
      <th>Z</th>
      <th>Xmax</th>
      <th>Zmax</th>
      <th>Xmin</th>
      <th>Zmin</th>
      <th>Xp</th>
      <th>Zp</th>
      <th>Xn</th>
      <th>Zn</th>
      <th>Zmaxn</th>
      <th>Zmaxn</th>
      <th>Xminn</th>
      <th>Zminn</th>
      <th>Xpn</th>
      <th>Zpn</th>
      <th>Raided</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tracks.each do |track| %>
      <tr>
        <td><%= track.player %></td>
        <td><%= track.x %></td>
        <td><%= track.z %></td>
        <td><%= track.xMax %></td>
        <td><%= track.zMax %></td>
        <td><%= track.xMin %></td>
        <td><%= track.zMin %></td>
        <td><%= track.xP %></td>
        <td><%= track.zP %></td>
        <td><%= track.xN %></td>
        <td><%= track.zN %></td>
        <td><%= track.zMaxN %></td>
        <td><%= track.zMaxN %></td>
        <td><%= track.xMinN %></td>
        <td><%= track.zMinN %></td>
        <td><%= track.xPN %></td>
        <td><%= track.zPN %></td>
        <td><%= track.raided %></td>
        <td><%= track.notes %></td>
        <td><%= link_to 'Show', track %></td>
        <td><%= link_to 'Edit', edit_track_path(track) %></td>
        <td><%= link_to 'Destroy', track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Track', new_track_path %>
